<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prabhattradingservice">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/application_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/application_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Abouts_Activty.Blogs"></activity>
        <activity android:name=".Abouts_Activty.Returns" />
        <activity android:name=".Abouts_Activty.Terms" />
        <activity android:name=".Abouts_Activty.Privacy_Policy" />
        <activity android:name=".Abouts_Activty.Disclaimer" />
        <activity
            android:name=".QueryForm"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MenuActivity.About"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MenuActivity.Training" />
        <activity android:name=".MenuActivity.Video" />
        <activity android:name=".MenuActivity.Gallery" />
        <activity android:name=".MenuActivity.Payment" />
        <activity android:name=".Abouts_Activty.About_us" />
        <activity
            android:name=".MenuActivity.Contact_Us"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Splash_Screen"
            android:theme="@style/AppTheme.NoActionBar1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ErrorScreen"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>